        "import pandas as pd\n",
        "\n",
        "# a. Create a DataFrame with Student, Marks, Attendance\n",
        "data = {\n",
        "    \"Student\": [\"Alice\", \"Bob\", \"Charlie\", \"David\", \"Eva\"],\n",
        "    \"Marks\": [78, 45, 88, 62, 91],\n",
        "    \"Attendance\": [85, 72, 90, 80, 95]\n",
        "}\n",
        "\n",
        "df = pd.DataFrame(data)\n",
        "\n",
        "print(\"a) DataFrame:\")\n",
        "print(df)\n",
        "print(\"\\nColumn names:\")\n",
        "print(df.columns)\n",
        "print()\n",
        "\n",
        "# b. Display first 3 rows, last 2 rows, and DataFrame shape\n",
        "print(\"b) First 3 rows:\")\n",
        "print(df.head(3))\n",
        "\n",
        "print(\"\\nLast 2 rows:\")\n",
        "print(df.tail(2))\n",
        "\n",
        "print(\"\\nDataFrame shape:\")\n",
        "print(df.shape)\n",
        "print()\n",
        "\n",
        "# c. Display students who scored more than 70\n",
        "print(\"c) Students who scored more than 70:\")\n",
        "print(df[df[\"Marks\"] > 70])\n",
        "print()\n",
        "\n",
        "# d. Create a Result column (Pass/Fail)\n",
        "df[\"Result\"] = df[\"Marks\"].apply(lambda x: \"Pass\" if x >= 50 else \"Fail\")\n",
        "\n",
        "print(\"d) DataFrame with Result column:\")\n",
        "print(df)\n",
        "print()\n",
        "\n",
        "# e. Save DataFrame to CSV and read it back\n",
        "csv_file = \"students_data.csv\"\n",
        "df.to_csv(csv_file, index=False)\n",
        "\n",
        "print(\"e) DataFrame saved to CSV.\")\n",
        "\n",
        "df_read = pd.read_csv(csv_file)\n",
        "print(\"\\nDataFrame read from CSV:\")\n",
        "print(df_read)\n"
      ],
