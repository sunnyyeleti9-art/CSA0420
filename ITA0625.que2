      "source": [
        "import numpy as np\n",
        "\n",
        "# a. Create a NumPy array containing marks of 10 students\n",
        "marks = np.array([78, 85, 92, 66, 74, 88, 90, 69, 81, 95])\n",
        "print(\"a) Marks array:\", marks)\n",
        "print(\"Shape:\", marks.shape)\n",
        "print(\"Data type:\", marks.dtype)\n",
        "print()\n",
        "\n",
        "# b. NumPy array from 10 to 100 with step size 10\n",
        "arr = np.arange(10, 101, 10)\n",
        "print(\"b) Array:\", arr)\n",
        "print(\"First element:\", arr[0])\n",
        "print(\"Last element:\", arr[-1])\n",
        "print(\"Elements from index 2 to 6:\", arr[2:7])\n",
        "print()\n",
        "\n",
        "# c. Element-wise addition and multiplication\n",
        "arr1 = np.array([1, 2, 3, 4, 5])\n",
        "arr2 = np.array([6, 7, 8, 9, 10])\n",
        "\n",
        "print(\"c) Array 1:\", arr1)\n",
        "print(\"Array 2:\", arr2)\n",
        "print(\"Element-wise addition:\", arr1 + arr2)\n",
        "print(\"Element-wise multiplication:\", arr1 * arr2)\n",
        "print()\n",
        "\n",
        "# d. Mask values greater than 50\n",
        "values = np.array([23, 45, 67, 89, 12, 54, 76, 38])\n",
        "mask = values > 50\n",
        "\n",
        "print(\"d) Original array:\", values)\n",
        "print(\"Mask (values > 50):\", mask)\n",
        "print(\"Values greater than 50:\", values[mask])\n",
        "print()\n",
        "\n",
        "# e. Reshape array into 3×4 matrix\n",
        "arr_reshape = np.arange(1, 13)\n",
        "reshaped = arr_reshape.reshape(3, 4)\n",
        "\n",
        "print(\"e) Original array:\", arr_reshape)\n",
        "print(\"Reshaped 3×4 array:\\n\", reshaped)\n"
      ]
    },
